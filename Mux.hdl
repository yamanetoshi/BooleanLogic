/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // (a And sel) Or (b And (Not sel))
    And(a=a, b=sel, out=tmpa);
    Not(in=sel, out=notsel);
    And(a=b, b=notsel, out=tmpb);
    Or(a=tmpa, b=tmpb, out=out);
}